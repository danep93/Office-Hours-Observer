<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.daniel.officehours">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Login"
            android:label="Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ProfessorMainActivity"
            android:label="ProfessorMainActivity" />
        <activity
            android:name=".Register"
            android:label="@string/title_activity_register" />
        <activity
            android:name=".StudentMainActivity"
            android:label="@string/title_activity_student_main"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".TAMainActivity"
            android:label="@string/title_activity_tamain"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".StudentClassAdder" />
        <activity android:name=".ClassCreator" />
        <activity android:name=".TAClassAdder"></activity>
    </application>

</manifest>
